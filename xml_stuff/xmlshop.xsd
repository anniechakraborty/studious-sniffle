			<xs:field xpath="."/>
		</xs:keyref>
		<xs:keyref name="FilmRefCompany" refer="DVDKey">
			<xs:selector xpath="./Companies/Company/Products/Films/Film"/>
			<xs:field xpath="."/>
		</xs:keyref>
		<xs:keyref name="MemberRef" refer="PersonKey">
