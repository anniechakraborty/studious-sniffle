	<xs:complexType name="PersonType">
		<xs:sequence>
			<xs:element name="Surname"/>
			<xs:element name="Forename" minOccurs="0"/>
			<xs:element name="Albums" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Album" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Books" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Book" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Films" minOccurs="0">
