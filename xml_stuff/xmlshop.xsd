					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:element name="Shop">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Books" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Book" type="BookType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CDs" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CD" type="CDType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DVDs" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DVD" type="DVDType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Persons" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Person" type="PersonType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
