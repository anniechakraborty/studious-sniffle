<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified">
	<xs:complexType name="ProductType" abstract="true">
		<xs:sequence>
			<xs:element name="Title" type="xs:string"/>
			<xs:element name="Year">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1950"/>
						<xs:maxExclusive value="2050"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Price">
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minExclusive value="0.0"/>
						<xs:totalDigits value="6"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Distributor" type="xs:IDREF"/>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="CDType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ProductType">
				<xs:sequence>
					<xs:element name="Genre" type="MusicGenre" default="None"/>
					<xs:choice>
						<xs:element name="Band" type="GroupType"/>
						<xs:element name="Soloartist" type="xs:IDREF"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GroupType">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="Member" type="xs:IDREF" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BookType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ProductType">
				<xs:sequence>
					<xs:element name="Genre" type="BookGenre" default="None"/>
					<xs:element name="Authors">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Author" type="xs:IDREF" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Editors" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Editor" type="xs:IDREF" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DVDType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ProductType">
				<xs:sequence>
					<xs:element name="Genre" type="FilmGenre" default="None"/>
					<xs:element name="RegionCode" default="2">
						<xs:simpleType>
							<xs:restriction base="xs:nonNegativeInteger">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="6"/>
								<xs:totalDigits value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Actors">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Actor" type="xs:IDREF" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Director" type="xs:IDREF"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CompanyType">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:choice>
				<xs:element name="Address" type="generalAddressType"/>
				<xs:element name="GermanAddress" type="GermanAddressType"/>
				<xs:element name="UKAddress" type="UKAddressType"/>
				<xs:element name="USAddress" type="USAddressType"/>
			</xs:choice>
			<xs:element name="Products" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Albums" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Album" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Books" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Book" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Films" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Film" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="AddressType" abstract="true">
		<xs:sequence>
			<xs:element name="Street" type="xs:string"/>
			<xs:element name="City" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="generalAddressType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="AddressType">
				<xs:sequence>
					<xs:element name="Zip" type="xs:string"/>
				</xs:sequence>
				<xs:attribute name="Country" type="xs:NMTOKEN"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GermanAddressType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="AddressType">
				<xs:sequence>
					<xs:element name="Postcode">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[0-9]{5}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Country" type="xs:NMTOKEN" fixed="DE"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="UKAddressType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="AddressType">
				<xs:sequence>
					<xs:element name="Postcode">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[A-Z]{1,2}[1-9]{1,2} [1-9][A-Z]{2}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Country" type="xs:NMTOKEN" fixed="UK"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="USAddressType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="AddressType">
				<xs:sequence>
					<xs:element name="State" type="USState"/>
					<xs:element name="Zip">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[0-9]{5}(-[0-9]{4})?"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Country" type="xs:NMTOKEN" fixed="US"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
